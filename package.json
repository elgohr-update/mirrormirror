{
  "name": "mirrormirror",
  "version": "1.0.2",
  "description": "An empathy machine for your digital self",
  "keywords": [
    "vr",
    "webxr",
    "threejs",
    "3d",
    "posenet",
    "tensorflow"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack serve --open --config webpack.dev.js",
    "start:mobile": "webpack serve --open --config webpack.dev.js --host 0.0.0.0 --https",
    "lint-js": "eslint --ext .js --fix src/**/*.js",
    "lint-style": "stylelint --fix 'src/**/*.{css,scss}'",
    "prettier": "pretty-quick --staged"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/florianporada/mirrormirror"
  },
  "author": "florianporada",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "favicons": "^6.2.0",
    "favicons-webpack-plugin": "5.0.0-alpha.7",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "husky": "^4.3.8",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^3.1.0",
    "purgecss": "^4.0.0",
    "sass-loader": "^10.1.1",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn run lint-js"
    ],
    "src/**/*.{css,scss}": [
      "yarn run lint-style"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently \"yarn run prettier\" \"lint-staged\""
    }
  },
  "dependencies": {
    "@tensorflow-models/posenet": "^2.2.1",
    "@tensorflow/tfjs": "^3.0.0",
    "@tensorflow/tfjs-backend-webgl": "^3.0.0",
    "@tensorflow/tfjs-core": "^3.0.0",
    "@tweenjs/tween.js": "^18.6.4",
    "cannon-es": "^0.16.0",
    "cannon-es-debugger": "^0.1.2",
    "dat.gui": "^0.7.7",
    "stats.js": "^0.17.0",
    "three": "^0.125.1"
  }
}
